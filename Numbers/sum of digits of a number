import java.util.Scanner;
public class number 
{
    public static void main(String[] args) throws Exception
    {
        Scanner sc = new Scanner(System.in);
        System.out.print("Enter the number : ");
        int num = sc.nextInt();
        int sum = 0;
        while(num!=0)
        {
            sum+=(num%10);
            num = num/10;
        }

        System.out.println("Sum of the digits of the numbe  is : " + sum);
    }
}
